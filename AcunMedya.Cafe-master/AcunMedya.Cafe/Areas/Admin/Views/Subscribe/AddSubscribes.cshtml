@model AcunMedya.Cafe.Entities.Subscribe

@{
    ViewData["Title"] = "AddSubscribes";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<div class="content-wrapper">
    <div class="page-header">
        <h3 class="page-title"> Abone Ekleme Tablsou </h3>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">Tablo</a></li>
                <li class="breadcrumb-item active" aria-current="page">Aboneler</li>
            </ol>
        </nav>
    </div>
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <form method="post" enctype="multipart/form-data">
                        <label>Id</label>
                        <input type="hidden" asp-for="SubscribeId" class="form-control" />
                        <br />
                        <label>Mail Adresi</label>
                        <input type="text" asp-for="SubscribeMail" class="form-control" />
                       

                        <br />
                        <button class="btn btn-primary"<i class="fa fa-save"></i> Kaydet</button>
                    </form>



                </div>
            </div>
        </div>

    </div>
</div>